{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "builder": "webpack",
    "tsConfigPath": "apps/admin-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin-api",
  "projects": {
    "admin-api": {
      "type": "application",
      "root": "apps/admin-api",
      "entryFile": "main",
      "sourceRoot": "apps/admin-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-api/tsconfig.app.json"
      }
    },
    "app-api": {
      "type": "application",
      "root": "apps/app-api",
      "entryFile": "main",
      "sourceRoot": "apps/app-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/app-api/tsconfig.app.json"
      }
    },
    "base": {
      "type": "library",
      "root": "libs/base",
      "entryFile": "index",
      "sourceRoot": "libs/base/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base/tsconfig.lib.json"
      }
    },
    "app-config": {
      "type": "library",
      "root": "libs/app-config",
      "entryFile": "index",
      "sourceRoot": "libs/app-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/app-config/tsconfig.lib.json"
      }
    },
    "content": {
      "type": "library",
      "root": "libs/content",
      "entryFile": "index",
      "sourceRoot": "libs/content/src",
      "compilerOptions": {
        "tsConfigPath": "libs/content/tsconfig.lib.json"
      }
    },
    "dictionary": {
      "type": "library",
      "root": "libs/dictionary",
      "entryFile": "index",
      "sourceRoot": "libs/dictionary/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dictionary/tsconfig.lib.json"
      }
    },
    "forum": {
      "type": "library",
      "root": "libs/forum",
      "entryFile": "index",
      "sourceRoot": "libs/forum/src",
      "compilerOptions": {
        "tsConfigPath": "libs/forum/tsconfig.lib.json"
      }
    },
    "system-config": {
      "type": "library",
      "root": "libs/system-config",
      "entryFile": "index",
      "sourceRoot": "libs/system-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/system-config/tsconfig.lib.json"
      }
    },
    "user": {
      "type": "library",
      "root": "libs/user",
      "entryFile": "index",
      "sourceRoot": "libs/user/src",
      "compilerOptions": {
        "tsConfigPath": "libs/user/tsconfig.lib.json"
      }
    }
  }
}
