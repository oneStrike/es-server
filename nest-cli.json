{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/admin-api/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/admin-api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/admin-api",
  "projects": {
    "admin-api": {
      "type": "application",
      "root": "apps/admin-api",
      "entryFile": "main",
      "sourceRoot": "apps/admin-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/admin-api/tsconfig.app.json"
      }
    },
    "base": {
      "type": "library",
      "root": "libs/base",
      "entryFile": "index",
      "sourceRoot": "libs/base/src",
      "compilerOptions": {
        "tsConfigPath": "libs/base/tsconfig.lib.json"
      }
    },
    "client-api": {
      "type": "application",
      "root": "apps/client-api",
      "entryFile": "main",
      "sourceRoot": "apps/client-api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/client-api/tsconfig.app.json"
      }
    },
    "client-config": {
      "type": "library",
      "root": "libs/client-config",
      "entryFile": "index",
      "sourceRoot": "libs/client-config/src",
      "compilerOptions": {
        "tsConfigPath": "libs/client-config/tsconfig.lib.json"
      }
    },
    "dictionary": {
      "type": "library",
      "root": "libs/dictionary",
      "entryFile": "index",
      "sourceRoot": "libs/dictionary/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dictionary/tsconfig.lib.json"
      }
    }
  }
}
