# 基础环境变量配置
# 数据库配置
DB_HOST=postgres
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=your_password
DB_NAME=es

# Redis配置
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=your_password
REDIS_NAMESPACE=ES
DATABASE_URL=postgresql://postgres:your_password@localhost:5432/es

# 阿里云短信服务配置
# AccessKey ID - 从阿里云 RAM 用户获取
ALIBABA_CLOUD_ACCESS_KEY_ID=your_access_key_id
# AccessKey Secret - 从阿里云 RAM 用户获取
ALIBABA_CLOUD_ACCESS_KEY_SECRET=your_access_key_secret
# 短信服务终端节点
ALIYUN_SMS_ENDPOINT=dypnsapi.aliyuncs.com
# 短信签名名称 - 需要在阿里云短信服务中申请
ALIYUN_SMS_SIGN_NAME=your_sign_name

# Apifox文档部署相关参数
APIFOX_API_KEY=your_api_fox_api_key
APIFOX_ADMIN_PROJECT_ID=your_admin_project_id
APIFOX_APP_PROJECT_ID=your_app_project_id
