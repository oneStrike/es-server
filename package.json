  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/audit(|/.*)$": "<rootDir>/libs/audit/src/$1",
      "^@libs/auth(|/.*)$": "<rootDir>/libs/auth/src/$1",
      "^@libs/base(|/.*)$": "<rootDir>/libs/base/src/$1",
      "^@libs/cache(|/.*)$": "<rootDir>/libs/cache/src/$1",
      "^@libs/captcha(|/.*)$": "<rootDir>/libs/captcha/src/$1",
