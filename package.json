{
  "name": "@akaiito/server-nestjs",
  "version": "0.0.2",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "description": "",
  "author": "",
  "license": "UNLICENSED",
  "scripts": {
    "reinstall": "rimraf ./node_modules && pnpm install && pnpm prisma:update",
    "build:admin": "cross-env NODE_ENV=production nest build admin-api",
    "build:app": "cross-env NODE_ENV=production nest build app-api",
    "start:dev": "",
    "start:admin": "nest build admin-api --webpack --webpackPath webpack.config.js --watch",
    "start:app": "nest build app-api --webpack --webpackPath webpack.config.js --watch",
    "start:debug": "cross-env NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/apps/admin-api/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit -p tsconfig.build.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/akaiito-server-nestjs/test/jest-e2e.json",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:init": "pnpm prisma:migrate:dev && pnpm prisma:seed",
    "prisma:reset": "prisma migrate reset",
    "prisma:pull": "prisma db pull",
    "prisma:seed": "prisma db seed",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:deploy": "pnpm prisma:migrate:deploy && pnpm prisma:seed",
    "prisma:update": "pnpm prisma:generate && pnpm prisma:migrate:dev && pnpm prisma:deploy",
    "publish-api:admin": "tsx publish-api.ts --app admin",
    "publish-api:app": "tsx publish-api.ts --app app"
  },
