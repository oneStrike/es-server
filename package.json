      "^@libs/database(|/.*)$": "<rootDir>/libs/database/src/$1",
      "^@libs/decorators(|/.*)$": "<rootDir>/libs/decorators/src/$1",
      "^@libs/dto(|/.*)$": "<rootDir>/libs/dto/src/$1",
      "^@libs/filters(|/.*)$": "<rootDir>/libs/filters/src/$1",
      "^@libs/health(|/.*)$": "<rootDir>/libs/health/src/$1",
