  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@libs/app-config(|/.*)$": "<rootDir>/libs/app-config/src/$1",
      "^@libs/base(|/.*)$": "<rootDir>/libs/base/src/$1",
      "^@libs/content(|/.*)$": "<rootDir>/libs/content/src/$1",
      "^@libs/dictionary(|/.*)$": "<rootDir>/libs/dictionary/src/$1",
      "^@libs/forum(|/.*)$": "<rootDir>/libs/forum/src/$1",
      "^@libs/system-config(|/.*)$": "<rootDir>/libs/system-config/src/$1",
      "^@libs/user(|/.*)$": "<rootDir>/libs/user/src/$1"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
