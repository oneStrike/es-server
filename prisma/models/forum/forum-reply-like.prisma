model ForumReplyLike {
  /// 主键ID
  id              Int       @id @default(autoincrement())
  /// 关联的回复ID
  replyId         Int       @map("reply_id") @db.Integer
  /// 关联的回复
  reply           ForumReply @relation(fields: [replyId], references: [id], onDelete: Cascade)
  /// 关联的用户ID
  userId          Int       @map("user_id") @db.Integer
  /// 关联的用户资料
  profile         ForumProfile @relation(fields: [userId], references: [id], onDelete: Cascade)
  /// 创建时间
  createdAt       DateTime  @default(now()) @map("created_at") @db.Timestamptz

  /// 索引优化
  @@unique([replyId, userId])
  @@index([replyId])
  @@index([userId])
  @@index([createdAt])
  @@map("forum_reply_like")
}
